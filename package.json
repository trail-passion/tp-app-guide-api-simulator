        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1",
        "webpack-shell-plugin-next": "^2.3.1",
        "workbox-webpack-plugin": "^6.5.4",
        "yaml-loader": "^0.6.0"
